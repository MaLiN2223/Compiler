double x = 0.1;
int i = 1;
while(i<a){
	int k = 2;
	bool isPrime = k;
	i = i + 2;
	while(k < i / 2){
		if(i % k == 0){
			isPrime = false;
		}
		k = k + 1 ;
		x = k - x;
	} 
	int z = 10;
	z = z + 1;
	for (z = 0; z < 10 ; z = z + 1){
		x = x +1;
	}
	if(isPrime){
		q = q + i;
	}
}